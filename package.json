    "coverage": "jest --coverage",
    "prepare": "husky",
    "format": "prettier --cache --write .",
    "format:check": "prettier --cache --check ."
  },
  "dependencies": {
    "cloudinary": "^2.7.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4"
  },
  "peerDependencies": {
    "@nestjs/common": ">7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/testing": "11.1.6",
    "@types/jest": "30.0.0",
    "@types/node": "24.8.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^9.1.7",
    "jest": "30.2.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.8.2",
    "ts-jest": "29.4.5",
    "tsup": "^8.5.0",
    "typescript": "5.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
