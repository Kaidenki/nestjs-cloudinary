{
  "name": "@kaidenki/nestjs-cloudinary",
  "version": "1.0.1",
  "description": "Cloudinary module for nestjs",
  "author": "kaidenki <kaidenki@proton.me>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "nestjs",
    "config",
    "cloudinary"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kaidenki/nestjs-cloudinary"
  },
  "bugs": "https://github.com/kaidenki/nestjs-cloudinary/issues",
  "readmeFilename": "README.md",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "jest --config ./test/jest.json --passWithNoTests",
